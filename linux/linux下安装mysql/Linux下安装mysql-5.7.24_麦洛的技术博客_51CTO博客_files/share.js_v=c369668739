//寻找所有code标签，加复制按钮鸭！
$(function () {
    isCodeCopy=false
    let copyType = $('.code-toolbar code').length > 0 ? 0 : 1
    var codes = copyType === 0 ? $('.code-toolbar code') : $('.pre-numbering');
    if (codes.length) {
        for (var i = 0; i < codes.length; i++) {
            $(codes[i]).parent('pre').wrap('<div class="hljs-cto"></div>')
            var code_id = "code_id_" + i;
            var btn = document.createElement("button");

            btn.setAttribute("data-clipboard-target", "#" + code_id);
            codes[i].setAttribute("id", code_id);
            if (typeof (uid) == 'undefined' || !uid) {
                btn.innerHTML = '登录后复制';
                btn.setAttribute("class", "copy_btn disable");
            } else {
                btn.innerHTML = '复制';
                btn.setAttribute("class", "copy_btn  copy" +i);
                var clipboard = new ClipboardJS('.copy'+i, {
                    target: function (e) {
                        isCodeCopy=true
                        let copyBody = copyType === 0 ? $(e).parent('.hljs-cto').find('pre').find('code') : $(e).parent('.hljs-cto').find('pre').find('.language-');
                        return copyBody[0]
                    }
                })
                clipboard.on('success', function (e) {
                    $(e.trigger).html('已复制')
                    isCodeCopy = false
                    initText($(e.trigger))
                    e.clearSelection();

                });

                clipboard.on('error', function (e) {
                    console.log(e);
                });
            }
            $(codes[i]).parent('pre').before(btn)
        }
        $('body').on('click', '.copy_btn', function (e) {
            // var that = $(this);
            // let copyBody=copyType===0?$(this).parent('.hljs-cto').find('pre').find('code'):$(this).parent('.hljs-cto').find('pre').find('.language-');
            try {
                if(typeof(uid) == 'undefined'||!uid){
                    Login({scene:'Copycode1'})
                    return;
                }
            }catch (error) {
                console.log(error, 'error');
            }

            // var txt = copyBody.html().replace(/<br>/g,'\r\n')
            // console.log(txt)
            // navigator.clipboard ? navigator.clipboard.write("text/plain",txt).then(copySuccess(that), function () { o(that) }) : o(that)
        })
        function o(e) {
            var t = document.createElement("textarea");
            let copyBody = copyType === 0 ? e.parent('.hljs-cto').find('pre').find('code') : e.parent('.hljs-cto').find('pre').find('.language-');
            t.value = copyBody.text();
            t.style.top = "0",
                t.style.left = "0",
                t.style.position = "fixed",
                document.body.appendChild(t),
                t.focus(),
                t.select();
            try {
                var o = document.execCommand("copy");
                setTimeout(function () { copySuccess(e) }, 1)
            } catch (t) {
                setTimeout(function () { copyEorr(e) }, 1)
            } document.body.removeChild(t)
        }
        function copySuccess(e) {
            e.html('已复制')
            initText(e)
        }
        function copyEorr(e) {
            e.html('复制失败')
            initText(e)
        }
        function initText(e) {
            setTimeout(function () {
                e.html('复制')
            }, 2000)
        }
    }
});

;
$(function() {
    var $data,methods,$onFun;
    var img_detail = {
        init: function() {
            $data = img_detail.data;$methods = img_detail.methods;$onFun = img_detail.onFun;
            $onFun()
        },
        onFun: function() {
            $(".imgViewDom").on("click", function() {
                var n = navigator.userAgent.toLowerCase();
                n.indexOf("windows ") > -1 ? $("body").css({
                    overflow: "auto",
                    "margin-left": "0"
                }) : $("body").css({
                    overflow: "auto"
                }),
                // $(".imgViewDom").fadeOut(500).children("img").attr("src", "")
                $(".imgViewDom").fadeOut(500).children('.img-content').html("")
            })
            $('.artical-content-bak img').bind("click", function(e) {
                var len = $(this).parents("a[href][href^='http']").length
                if(len) return
                var c_height = e.currentTarget.naturalHeight
                var w_height = $(window).height()
                var c_img = c_height < w_height ? '<img class="small-img" src="">' : '<img src="">'
                $(".imgViewDom .img-content").html(c_img)
                e.currentTarget.src && ($(".imgViewDom").fadeIn(500).children('.img-content').children("img").css({
                    width: e.currentTarget.naturalWidth,
                    height: e.currentTarget.naturalHeight
                }).attr("src", e.currentTarget.src),$methods.controlBodOverflow())
            })
            $('.artical-content-bak .cto-mermaid svg').bind("click", function(e) {
                var svg_height = $(this).attr('height')
                var w_height = $(window).height()
                if(svg_height < w_height) $(this).addClass('small-img')
                var t1 = e.currentTarget.cloneNode(true)
                $(".imgViewDom .img-content").html(t1)
                $(".imgViewDom").fadeIn(500)
                $methods.controlBodOverflow()
            })
        },
        methods: {
            controlBodOverflow: function() {
                var n = navigator.userAgent.toLowerCase()
                n.indexOf("windows ") > -1 ? $("body").css({
                    overflow: "hidden",
                    "margin-left": "-17px"
                }) : $('body').css({
                    overflow: "hidden"
                })
            }
        },
    }
    img_detail.init()
})
;
// QQ分享地址
function share(type, url, title, pics, desc) {
    var share = {
        share_url: url?url:window.location.href,
        title: title ? title : document.title,
        pics: pics ? pics : 'https://static2.51cto.com/edu/blog/mobile/images/share_default.jpg',
        desc: desc ? desc : document.getElementsByName('description')[0].content,
        appkey: 0
    };
    switch (type) {
        case 'qq':
            window.open("https://connect.qq.com/widget/shareqq/index.html?url=" + encodeURIComponent(share.share_url) + "&title=" + share.title + "&desc=" + share.desc + "&pics=" + share.pics)
            break;
        case 'weibo':
            window.open("https://service.weibo.com/share/share.php?url=" + encodeURIComponent(share.share_url) + "&title=" + share.title + "&pic=" + share.pics + "&appkey=" + share.appkey + "&searchPic=true");
            break;
        case 'weixin':
            
            break;
        default:
            window.open("https://connect.qq.com/widget/shareqq/index.html?url=" + encodeURIComponent(share.share_url) + "&title=" + share.title + "&desc=" + share.desc + "&pics=" + share.pics)
            break;
    }
}
$('.bds_tsina').click(function(){
    share('weibo');
});
$('.bds_sqq').click(function(){
    share('qq');
});
$('.bds_tsina_success').click(function(e){
    share('weibo', $(this).attr('data-url'));
});
$('.bds_sqq_success').click(function(e){
    share('qq', $(this).attr('data-url'));
});
;
