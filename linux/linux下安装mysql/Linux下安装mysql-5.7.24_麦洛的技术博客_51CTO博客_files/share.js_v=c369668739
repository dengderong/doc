getUserStat(blog_id)
getInfoStat(blog_id)
getDatelist(blog_id)
function getUserStat(id){//用户数据
    $('#userStat').append('<img src="'+img_url+'images/loading.gif" style="margin: 0 auto;display:block;">');	
    $.ajax({
        url: BLOG_URL + 'blog/user-stat',
        type: 'get',
        dataType: 'json',
        data:{id:id},
        success: function (res) {
            if (res.status === 0) {  
                var htmlData = template('userStatScript', res.data);
                $('#userStat').html(htmlData);												
            }
        }
    })
}
function getInfoStat(id){//博文统计
    $.ajax({
        url: BLOG_URL + 'blog/info-stat',
        type: 'get',
        dataType: 'json',
        data:{id:id},
        success: function (res) {
            if (res.status === 0) {  
                $('.praise-num').text(res.data.apraise>0 ? res.data.apraise : '')													
                $('.favorites-num').text(res.data.favorite>0 ? res.data.favorite : '')
                $('#CommentNum,.comments_num').text(res.data.comments>0 ? res.data.comments : '')
                if(res.data.pv != ''){
                    $('.article-detail .mess-tag').append('<strong class="fl"><em class="fl">阅读数</em><b class="fl">'+res.data.pv+'</b></strong>')
                }
            }
        }
    })
}
function getDatelist(id){//时间轴
    $('#Datelist').append('<img src="'+img_url+'images/loading.gif" style="margin: 0 auto;display:block;">');	
    $.ajax({
        url: BLOG_URL + 'blog/datelist',
        type: 'get',
        dataType: 'json',
        data:{id:id},
        success: function (res) {
            if (res.status === 0) { 
                if(res.data&&Array.isArray(res.data)&&res.data.length==0){
                    $('#Datelist').remove();
                }else{
                    var htmlData = template('datelistScript', res.data);
                    $('#Datelist').html(htmlData);
                }										
            }
        }
    })
}

function base64ToBlob(base64Data) {
    var arr = base64Data.split(','),
        fileType = arr[0].match(/:(.*?);/)[1],
        bstr = atob(arr[1]),
        l = bstr.length,
        u8Arr = new Uint8Array(l);
        
    while (l--) {
        u8Arr[l] = bstr.charCodeAt(l);
    }
    return new Blob([u8Arr], {
        type: fileType
    });
};
// blob转file
function blobToFile(newBlob, fileName) {
    newBlob.lastModifiedDate = new Date();
    newBlob.name = fileName;
    return newBlob;
};

function selectImg(file) {
    // console.log(111)
    if (!file.files || !file.files[0]) {
        return;
    }
    //定以一个读取文件的对象
    var reader = new FileReader();

    reader.onload = function (evt) {
        //获取的是图片的base64代码
        var replaceSrc = evt.target.result;
        // console.log('replaceSrc-----', replaceSrc)

        //定义一个Image对象
        var image = new Image();
        image.src = replaceSrc;
        // 再将获取值赋给img标签
        $('#report-img').attr("src", replaceSrc);
        $('.report-img_rule').hide();
        $('.report-img-container').hide();
        $('.report-img-show').show();
        $('.repot-btn-select').hide();
    };
    reader.readAsDataURL(file.files[0]);
    $('.repot-btn-select').replaceWith('<input type="file" accept="image/jpg,image/jpeg,image/png" name="file" class="repot-btn-select" onchange="selectImg(this)"></input>');
}

