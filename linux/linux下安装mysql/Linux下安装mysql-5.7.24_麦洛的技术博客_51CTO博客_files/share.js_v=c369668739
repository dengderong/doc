    ScrollTopFn:function(b){
        if(b.length==0)return false
        var scrollTop = b.offset().top-90
        if(scrollTop > 0){
            $("html,body").animate({scrollTop:scrollTop},'slow',function(){$(this).stop(true,true);});
        }
    },
    Ahref: function(obj,id){
        $('body').on('click', obj, function(){
            if($(id) != 'undefined'){
                var scrollTop = $(id).offset().top
                scrollTop>0?$(window).scrollTop(scrollTop-90):""
            }
        })
    },
    AddAnimate: function(obj){
        obj.append('<font class="jia2">+1</font>').find('font').animate({top:'-16px',right:'4px',opacity:'0'},300,'swing',function(){$('.jia2').fadeOut(500)})
    },
    ScrollPopover:function(){
        var self = this;
        var initialHeight = $(document).scrollTop();
        var w_height = $(window).height();
        $(document).scroll(function(){
            var scroH = $(document).scrollTop();
            if((scroH - initialHeight >w_height ||scroH - initialHeight < -w_height) && isLogin != 1){
                self.Login()
                $(document).unbind("scroll")
            }
        })
        
    },
    ScrollWindowTop:function(){
        $('.ScrollWindowTop').click(function(){
            $(window).scrollTop(0) 
            $('.fixtitle').hide();
            $('.Header,.home-top').show();
        })        
    },
    ScrollHeader:function(){
        var scrolltop = new Array();
        var index = 0;
        scrolltop[0] = 0;
        $(document).scroll(function(){
            if($(document).scrollTop()>500){           
                index ++;
                scrolltop[index] = $(document).scrollTop();
                if (scrolltop[index] > scrolltop[index-1]) {
                    $('.fixtitle').show();
                    $('.Header,.home-top').hide();
                }else{
                    $('.fixtitle').hide();
                    $('.Header,.home-top').show();
                };
            }
            if($(window).width()<=1440){        
                if( $(document).scrollTop() > $('#asideoffset').offset().top-100  ){
                    $('.action-aside-left').hide();
                    $('body').removeClass('body_padding')
                }else{
                    if(!$('.detail-content-left').hasClass('set-right')){//博文沉浸式阅读重新绘制了
                        $('.action-aside-left').show();
                        $('body').addClass('body_padding')
                    }                    
                }
            }
        })    
    },
    ShowCopyright:function(){
        $('#copyright-btn').click(function(){
            $('.copytext').slideToggle(200);
        })
    },
