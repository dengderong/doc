// 展示代码行数
var prenumber = function(classStrArr) {
    classStrArr.map(function(item) {
        $('.' + item + ' pre').addClass("prettyprint")
    })
    $("pre.prettyprint code").each(function() {
        var deleten = $(this).text().replace(/(\n+)$/g,"")
        var t = deleten.split("\n").length , e = $("<ul/>").addClass("pre-numbering").hide();
        $(this).addClass("has-numbering").parent().append(e)
        for (let i = 1; i <= t; i++)
            e.append($("<li/>").text(i+"."));
        e.fadeIn(1500)
    })
}
// 展示pre行号
var addPreNumber = function() {
    var preArr = $('.editor-preview-side pre:not(:has(code))')
    if(preArr.length == 0) return;
    preArr.each(function() {
        var deleten = $(this)[0].innerText.replace(/(\n+)$/g,"")
        var t = deleten.split("\n").length , e = $("<ul/>").addClass("pre-numbering").hide();
        $(this).addClass("has-pre-numbering").append(e)
        for (let i = 1; i <= t; i++)
            e.append($("<li/>").text(i+"."));
        e.fadeIn(1500)
    })
}
;
// zjf20171011
var  version_Js = 20211104//更新js版本号
var IntroduceFn1 = function(){this.Init()}
IntroduceFn1.prototype = {   
    Init: function(){
        var _this = this
        _this.LoadPageParms = {
            blog_id: blog_id,
            order_field: 'create_time',
            order:0,
            page: cid != 0 ? page : 1,//cid 通过链接上cid参数直接访问该条评论所在的当页
            page_size:10,
        };
        _this.page_isClick = 0;
        _this.timer1;
        _this.Sort()            //排序
        _this.Favorites()       //收藏
        _this.Praise()          //文章点赞
        _this.Report()          // 举报
        _this.ReportSubmit()    // 提交举报信息
        _this.ReportCancel()    // 取消举报
        _this.LoadPage(_this.LoadPageParms)        //分页
        _this.SetPageClick() 
        _this.showMore()
        _this.SetReplay()       //发表评论
        _this.SetReply()        //回复评论
        _this.Remove()          //删除
        _this.Focus()
        _this.Ahref('.Topraise','#comment')
        _this.Ahref('.comment-num','#comment')
        _this.setPraise($('.comment-List .vote'))//评论点赞
        _this.ScrollWindowTop();
        _this.ShowCopyright();
        _this.ToSx();
        _this.ScrollHeader();
        _this.saveReply();
        _this.stoptextareaPropagation();
        _this.setAlink() //超链接处理
        _this.handleReportType()  // 选择举报类型
        _this.handleReportReasons() // 选择举报原因
        _this.handleFontCount(); // 控制补充说明右下角计数
        _this.handleUploadImg(); // 上传截图
        _this.handleDeleteImg(); // 删除已经上传的图片
        _this.handleCloseMaskDialog(); // 点击遮罩关闭弹窗
        _this.handleStopClick(); // 阻止事件冒泡
    },
    Focus: function(){
        var _this = this
        $('.textarea-show').click(function(e){
            if(isLogin != 1) {
                Login({scene:'comment1'})
            }else{
                e.stopPropagation();
                $('.textarea-show').hide()
                $('.textarea-hide').show()
                $('.textarea-hide textarea').focus();
            }
        })
    },
    stoptextareaPropagation:function(){
        $('#textareadiv-publish').click(function(e){
            e.stopPropagation();
        })
    },
    SetReplay: function(){//发表评论参数
        var _this = this
        $('.publish-btn').unbind()
        $('.publish-btn').click(function(e){
            e.stopPropagation();
            var me = $(this)
            var flag = me.attr('flag') // 1评论 2回复
            var content = me.parent().parent().find('.textareadiv').val()
            var first_pid = flag==1 ? 0 : me.parents('.comment-List').find('.first_pid').val()
            var parent_id = flag==1 ? 0 : me.parents('.comment-List').find('.parent_id').val()
            var reply_uid = flag==1 ? 0 : me.parents('.comment-List').find('.reply_uid').val()
            var data = {
                'blog_id': blog_id,
                'content': $.trim(content),
                'parent_id': parent_id,
                'first_pid': first_pid,
                'reply_uid':reply_uid,
            }
            _this.ReplayAjax(data,me)
       })
    },
    SetReply: function(){
        var _this = this
        $('.reply').unbind("click")
        $('.reply').click(function(){//回复
            var me = $(this)
            _this.SetReplyCon(me)
        })
    },
    SetReplyCon: function(me){
        var _this = this
        var placeholder_txt = '@'+me.parents('.comment-detail').find('h3 a').text();
        var reply_text = $('<div class="textarea-hide"><div class="top"><textarea class="textareadiv textareadiv-publish" name="" id="" placeholder="'+placeholder_txt+'" maxlength="500"></textarea></div><div class="bot clearfix"><strong class="fr publish-btn" flag="2">发布评论</strong><span class="fr cancel-btn-2">取消</span></div></div>');
        if(isLogin != 1) {
            Login({scene:'comment2'})
            return false;
        }
        $('.comment-List').find('.textarea-hide').remove();
        if(me.parents('.comment-detail').find('.textarea-hide').length == 0){
            me.parents('.comment-detail').append(reply_text)
        }        
        _this.CancelBtn()       //取消回复
        _this.SetReplay()       //发表评论
        _this.Remove()          //删除
        _this.SetPageClick()    //click page
        setTimeout(function(){
            reply_text.find('textarea').focus()
        },250)
    },
    SetPageClick: function() {
        var _this = this;
        $(".pagination-ajax li a").click(function(e) {
            _this.LoadPageParms.page = parseInt($(this).attr('data-page'))+1         
            if(!$(this).parent('li').hasClass('active')){		
                e.preventDefault();
                _this.page_isClick = 1 //点击了分页，地址上的cid定位失效
                _this.LoadPage(_this.LoadPageParms);
                _this.ScrollTopFn($('#comment'))
            }              
        })
    },
    Sort: function(){//评论排序
        var _this = this
        $(".comment-num .sort").click(function(e) {
            _this.is_load = false;
            $(this).addClass('on').siblings('.sort').removeClass('on')
            _this.LoadPageParms.order_field = $(this).attr('type')
            _this.LoadPageParms.order = $(this).attr('order')
            _this.LoadPage(_this.LoadPageParms);
        })
    },
    LoadPage: function(parms){//分页
        var _this = this
        if(_this.is_load)return false;
        $(".comment-List-box").html('<img src="'+img_url+'images/loading.gif" style="margin: 200px 370px;">');
        _this.is_load = true;       
        $.get(comment_list,parms,function(e){
            _this.is_load = false;
            if(e.code == 0){   
                if(e.data.total>0){
                    var htmlData = template('commentListScript', e.data);
                    $(".comment-List-box").html(htmlData);
                    $('#comment .comment-num').show()
                    $('#CommentNum,.comments_num').text(e.data.total)
                }else{
                    $('#comment .comment-num').hide()
                    $('.comments_num,#CommentNum').text('')
                    $(".comment-List-box").html('');
                }            
                _this.setPraise($('.comment-List .vote'))//点赞
                _this.SetPageClick()    //click page
                _this.CancelBtn()       //取消回复
                _this.SetReplay()       //发表评论
                _this.SetReply()        //回复评论
                _this.Remove()          //删除
                _this.showMore()
                if (cid && _this.page_isClick == 0){ //通过链接地址定位某一页评论
                    _this.ScrollTopFn($('#'+cid))
                }
            } else {
                new AutoBox({content:e.msg,autoClose:3})
            }
        },'json')
    },
    showMore:function(){
        $('body').on('click','.more-toggle',function(){           
            $(this).parent('.floor-2').find('.comment-List').slideDown();
            $(this).hide();
        })
    },
    ReplayAjax: function(data,dom){//评论ajax
        var _this = this
        if(isLogin != 1) {
            Login({scene:'comment2'})
            return false;
        }
        if(data.content.length==0){
            new AutoBox({content:'评论内容不能为空~',mask:"#000",autoClose:3})
        }else{
            $.ajax({
                url: addReply_url,
                data: data,
                type: 'POST',
                dataType: 'JSON',
                success: function(e){
                    if(e.code == 0){                        
                        _this.LoadPage(_this.LoadPageParms) 
                        if(dom.attr('flag') == 1){//评论
                            _this.ScrollTopFn($('#comment'))   
                        }else{//回复
                           setTimeout(function(){
                            var pid = dom.parents('.comment-List').find('.first_pid').val();
                            _this.ScrollTopFn($('#'+pid))  
                           },300)
                        }                
                        $('.textareadiv-publish').val('')
                        $('.comment-textarea .textarea-show span').text('提问和评论都可以，用心的回复会被更多人看到');
                        $('.comment-textarea .textarea-show').show();
                        $('.comment-textarea .textarea-hide').hide();
                        if($('.detail-content-left').hasClass('set-right')){//博文沉浸式阅读重新绘制了
                            $('#page_center').height($('.detail-content-left').height())
                        }  
                        _this.openDalog()                      
                    }else if(e.code == 10005){
                        new AutoBox({W:360,content:'<div style="padding: 0 40px;">很抱歉，您的账号因违规已被关闭，不能发布评论。如果您有疑问，请联系<span class="off-zhiCustomBtn" style="color:#3e71f6; cursor: pointer;">在线客服</span></div>',mask:"#000"})
                    }else if(e.code == 10004){
                        Login({scene:'comment2'})
                    }else{
                        new AutoBox({content:'<div style="padding: 0 40px;">'+e.msg+'</div>',mask:"#000",autoClose:3})
                    }
                }
            })
        }
    },
    CancelBtn: function(){//取消回复
        $('.cancel-btn-2').click(function(){
            $(this).parents('.textarea-hide').remove();
        })
    },
    saveReply:function(){
        $('body').click(function(e){
            if(!$('.comment-textarea .textarea-hide').is(':hidden')){
                if(e.target.id != 'textareadiv-publish' || e.target.id != 'publish-btn'){
                    $('.comment-textarea .textarea-show').show();
                    $('.comment-textarea .textarea-hide').hide();
                    if($('#textareadiv-publish').val()!= ''){
                        $('.comment-textarea .textarea-show span').text($('#textareadiv-publish').val())   
                    }else{
                        $('.comment-textarea .textarea-show span').text('提问和评论都可以，用心的回复会被更多人看到') 
                    }                   
                }
            }
            
        })
    },
    openDalog: function (){
        var _this = this
        var mask = $('<div class="mask"></div>')
            msg_index_box = $('<div class="msg-index-box"></div>'),
            cls = $('<p class="follow-close"></p>'),
            top_bg = $('<p class="top-bg"></p>'),
            con_box = $('<div class="con-box"></div>'),
            icon_1 = $('<p class="icon-bg icon-1"></p>')
            icon_2 = $('<p class="icon-bg icon-2"></p>')
            h2 = $('<h2>发布成功</h2>')
            con = $('<div class="con">关注并绑定官方微信服务号可随时随地</div><div class="con">收到回复提醒</div>'),
            btn_box = $('<div class="btn-box"><p class="btn-1">不再提醒</p><p class="btn-2">知道了</p></div>')

            if(getCookie('follow1') && getCookie('follow2'))return false;

            $.get('/wechat-ajax/qrcode', function (res) {
                if (res.code == 0) {
                    var data = res.data
                    if (!data.qr_code) {
                        if(getCookie('follow2'))return false;
                        msg_index_box.remove()
                        $('body').append(mask).append(msg_index_box)
                        msg_index_box.append(cls).append(top_bg).append(con_box)
                        con_box.html('').append(icon_2).append(h22).append(con1).append(btn_box)
                        msg_index_box.css('height','150px')
                    } else {
                       if(getCookie('follow1'))return false;
                        qrcodeSid = data.sid
                        var code_bg = '<div class="code-bg"><img src="' + data.qr_code + '" ></img></div>'
                        msg_index_box.remove()
                        $('body').append(mask).append(msg_index_box)
                        msg_index_box.append(cls).append(top_bg).append(con_box)
                        con_box.html('').append(icon_1).append(h2).append(code_bg).append(btn_box)
                        msg_index_box.css('height', '250px') 
                    }
                    $('.mask').show()
                    $('.btn-box .btn-2, .msg-index-box .follow-close').bind('click', function () {
                        $('.msg-index-box').remove()
                        $('.mask').remove()
                        clearInterval(_this.timer1)
                    })
                    $('.btn-box .btn-1').bind('click', function () {
                        if (!data.qr_code) {
                            setCookie('follow2','follow2',3650)
                        } else {
                            setCookie('follow1','follow1',3650)
                        }
                        $('.msg-index-box').remove()
                        $('.mask').remove()
                        clearInterval(_this.timer1)
                    })
                    // $("body").on('click',".code-bg img",function(){
                    //     if (_this.timer1) {
                    //         return false
                    //     }
                    //     $.get('/wechat-ajax/qrcode', function (res) {
                    //     if (res.code == 0) {
                    //         clearInterval(_this.timer1)
                    //         qrcodeSid = res.data.sid
                    //         $(".code-bg img").attr("src", res.data.qr_code)
                    //         polling()
                    //     }
                    // }, 'json')
                    // })
                    // 轮循
                    data.qr_code && polling()
                }
            }, 'json')
    },
    Remove: function(){//删除回复
        var _this = this
        $('.comment-List .delete').unbind("click");
        $('.comment-List .delete').click(function(){
            var _me = $(this)
            var id = _me.parents('.comment-List').find('.parent_id').val();
            _this.isLogin()
            new AutoBox({content:'<div style="padding: 0 30px 30px 30px;">是否要删除发布的评论？</div>',img:'remind',mask:"#000",Yes:'确定',No:'取消',
                yc:function(){   
                    $.post(removeUrl,{blog_id: blog_id,id: id},function(e){
                        if(e.code == 0){        
                            _this.LoadPage(_this.LoadPageParms)                    
                            if($('.detail-content-left').hasClass('set-right')){//博文沉浸式阅读重新绘制了
                                $('#page_center').height($('.detail-content-left').height())
                            }
                        }else{
                            new AutoBox({content:e.msg,img:'remind',mask:"#000",autoClose:3})
                        }
                    },'json')
                }
            })
        })
    },
    setPraise: function(obj){//赞
        obj.click(function(ev){
            var me = $(this),
                text = parseInt(me.find('b').attr('num')),
                blog_id =  me.parents('.comment-List').find('.parent_id').val()
                userid = me.parents('.comment-List').find('.reply_uid').val()
            if(me.hasClass('on')){
                new AutoBox({content:'您已点赞过',mask:"#000",autoClose:2})
                return false;
            }
            if(isLogin == 1){//登录
            $.post(praise_url,{id:blog_id,type:2,userid:userid},function(e){
                if(e.status == 1){                
                    me.addClass('on')
                    me.find('b').text(text+1)
                    var x = $('<div class="jia1">+1</div>')
                    .css({'top':ev.pageY-15,'left':ev.pageX+6,'z-index':10})
                    $("body").append(x)
                    x.animate({top:ev.pageY-20,left:ev.pageX+10,'font-size':'16px',opacity:'show'},300,'swing',function(){
                        x.fadeOut(500)
                    })    

                }else{
                new AutoBox({content:e.msg,img:'remind',mask:"#000",autoClose:3})
                }
            },'JSON')
            }else{//没登录
            Login({scene:'LikeButton2'})
            }
        })
    },
    Favorites: function(){//收藏
        var _this = this
        $('.favorites-opt').click(function(){
            var me = $(this)
                bVal = me.find('.favorites-num').text()!='' ? parseInt(me.find('.favorites-num').text()) : 0;
            if(isLogin != 1){
                Login({scene:'house1'})
                return false;
            }
            if(me.hasClass('on')){
                new AutoBox({content:'您已收藏过',mask:"#000",autoClose:2})
                return false;
            }
            //更新状态
            $('.favorites-opt').addClass('no-favorites on') 
            var params = {
                data_id:blog_id,
                data_type:0,
                url:window.location.href
            }
            $.ajax({
                    url: collect_url,
                    type: 'POST',
                    dataType: 'json',
                    data:params,
                    success: function (res) {
                        if(res.code == 0){//成功
                            $('.favorites-num').text(bVal+1)                                   
                        }else{//其他
                            $('.favorites-opt').removeClass('no-favorites on')
                            new AutoBox({content:res.msg,mask:"#000",autoClose:3})
                        }
                    }
                })
        });
    },
    Praise: function(){//赞
        var _this = this
        $('.is-praise').click(function(ev){
            var me = $(this),
                text = parseInt(me.find('.praise-num').text() == '' ? 0 : me.find('.praise-num').text()),
                type = me.attr('type'),
                blog_id = me.attr('blog_id'),
                userid = me.attr('userid');
            if(isLogin != 1){
                Login({scene:'LikeButton1'})
                return false;
            }
            if(me.hasClass('on')){
                new AutoBox({content:'您已点赞过',mask:"#000",autoClose:2})
                return false;
            }
            //更新状态
            $('.is-praise').addClass('no-praise on')
            $.post(praise_url,{id:blog_id,type:type,userid:userid},function(e){
                if(e.status == 1){
                    $('.is-praise .praise-num').text(text+1)
                }else{
                    $('.is-praise').removeClass('no-praise on')
                    new AutoBox({content:e.msg,img:'remind',mask:"#000",autoClose:3})
                }
            },'JSON')
        })
    },
    Report: function() { // 举报
        var _this = this;
        $('.article-report').click(function() {
            if(isLogin != 1){
                Login({scene:'writetostate1'})
                return false;   
            }
            $('.Mask').show();
            $('.report-dialog-root').show();
            $('.report-dialog-container').show();
        })
    },
    // 提交举报信息
    ReportSubmit: function() {
        var _this = this;
        $('.report-btn-concert').click(function() {
            var reportType = $('#report-specific-type').attr('data-type');
            var reportReasons = $('#report-specific-reasons').attr('data-type');
            var reportOriginalLink = $('.report-orgin_input').val().trim();
            var reportMoreInfo = $('#report-more-info').val().trim();
            var reportImg = $('#report-img').attr('src');
            var isCanSubmit = true;
            var myTimer = null;
            if(reportType == 0) {
                $('.report-warm-tip').text('请选择举报类型');
                isCanSubmit = false;
            }
            if((reportType == 1) && (reportReasons == 0)) {
                $('.report-warm-tip').text('请选择具体原因');
                isCanSubmit = false;
            }
            if((reportType == 3) && (!reportOriginalLink)) {
                $('.report-warm-tip').text('请正确输入原文链接');
                isCanSubmit = false;
            }
            if((reportType == 5) && (!reportMoreInfo)) {
                $('.report-warm-tip').text('请填写补充说明');
                isCanSubmit = false;
            }
            if(!isCanSubmit) {
                $('.report-warm-tip').show();
                setTimeout(function(){
                    $('.report-warm-tip').hide();
                }, 2000);
                return;
            }
            var subParams = {
                blog_id: blog_id,
                reason_type: reportType,
                reason_detail_type: reportReasons,
                original_url: reportOriginalLink,
                more_info: reportMoreInfo,
                image_url: ""
            }
            $('.report-btn-concert').attr('disabled', true);
            function submitReportData(data) {
                $.ajax({
                    type: 'POST',
                    url: blog_url+'/blogger-ajax/report',
                    data: subParams,
                    success: function(res) {
                        if(res.status == 1) {
                            $('.report-warm-tip').text('已经收到您得举报信息，我们会尽快审核');
                            $('.report-warm-tip').show();
                        }
                        if(res.status == 0) {
                            $('.report-warm-tip').text(res.msg);
                            $('.report-warm-tip').show();
                        }
                        setTimeout(function(){
                            $('.report-warm-tip').hide();
                            _this.handleInitReportInfo();
                        }, 2000);
                    },
                    error: function(error){
                        $('.report-warm-tip').text(error + ',请稍后重试');
                        $('.report-warm-tip').show();
                        setTimeout(function(){
                            $('.report-warm-tip').hide();
                            _this.handleInitReportInfo();
                        }, 2000);
                    }
                });
            }

            if(reportImg) {
                var blob = base64ToBlob(reportImg);
                var imgFile = blobToFile(blob, '123');
                // console.log('imgFile----', imgFile);
                function callbackRes(res) {
                    // console.log('tupian----', res);
                    subParams.image_url = res;
                    submitReportData(subParams);
                }
                uploadNow(imgFile,callbackRes);
            }else {
                submitReportData(subParams);
            }    
        })
    },
    // 取消举报
    ReportCancel: function() {
        var _this = this;
        $('.report-btn-cancel').click(function() {
            _this.handleInitReportInfo();
        })
    },
