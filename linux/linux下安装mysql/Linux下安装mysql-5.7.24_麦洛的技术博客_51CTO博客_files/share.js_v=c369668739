// 获取图片上传需要的sign值
function getImageUploadInfo() {
    if(window.infoOption) {
        return infoOptionBack = window.infoOption
    }
    $.ajax({
        type: "GET",
        url: blog_url+"/blogger/upload-sign",
        dataType: "json",
        timeout: 5000,
        success: function(res) {
            infoOptionBack = res.data;
            // console.log('infoOptionBack-----', infoOptionBack);
        },
        error: function(error){
            // console.log(error)
        }
    })
}
if(userId!=''){
