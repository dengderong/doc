            }
        })
    },
    setAlink:function(){
        //var reg = /^http(s)?:\/\/([\w-]+\.)+[\w-]+(\/[\w- ./?%&=]*)?$/
        var regUrl = /^((http(s)?:)?\/\/)?((\w+-?)?\w+\.)*51cto.com(\/.*)*$/;
        var flags = false;        
        //$('.editor-preview-side a:not([href^="#"])').attr('target', "_blank") 
        $('.editor-preview-side a:not([href^="#"])').click(function(e){
            e.preventDefault() 
            var me = $(this)
            var url = me.attr('href');
            if(typeof (url) != 'undefined'){                           
                if(flags == true) return false;
                flags = true;                                
                if(!regUrl.test(url)){  
                    if(url=='http://love51cto.mikecrm.com/6TVXoS6'){
                        window.open(url)
                    }else{
                        window.open(blog_url+'transfer?'+url)   
                    }                         
                }
                else{
                    window.open(url)
                }               
                setTimeout(function(){
                    flags = false;
                },1000);
            }
        })  
        $('.editor-preview-side a').each(function(){
            var me = $(this)
            var url = me.attr('href');
            if(typeof (url) != 'undefined'){ 
                if($(this).find('img').length == 0){
                    $(this).prepend('<i class="iconblog bloglink" style="font-size:inherit;"></i>&nbsp;')
                } 
            }          
        })
    }
}
